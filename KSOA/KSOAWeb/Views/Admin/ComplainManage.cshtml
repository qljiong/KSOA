@using KSOA.Model;
@model List<ComplainAnalysisList>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Scripts/ui/skins/Aqua/css/ligerui-all.css" rel="stylesheet" />
    <link href="~/Content/Admin/Admin.css" rel="stylesheet" />
    <link href="~/Content/Admin/pagination.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.3.2.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/messages_cn.js"></script>
    <script src="~/Scripts/ui/js/ligerBuild.min.js"></script>
    <script src="~/Scripts/Admin/function.js"></script>
    <title>ComplainManage</title>
</head>
<body class="mainbody">
    <form id="form1">
        <div class="tools_box">
            <div class="tools_bar">
                <div class="search_box">
                    <input type="text" ID="txtKeywords" Class="txtInput" />
                    <input type="button" ID="btnSearch" value="搜 索" Class="btnSearch" />
                </div>
                <a href="../admin/ImportExcelByComplain" class="tools_btn"><span><b class="add">导入投诉源数据</b></span></a>
                @*<a href="javascript:void(0);" onclick="checkAll(this);" class="tools_btn"><span><b class="all">全选</b></span></a>
                    <a href="javascript:void(0);" id="btnDelete" class="tools_btn"
                       onclick="return ExePostBack('btnDelete', '', 'cp');"><span><b class="delete">批量删除</b></span></a>*@
            </div>
        </div>

        <!--列表展示.开始-->
        <table width="100%" border="0" cellspacing="0" cellpadding="0" class="msgtable">
            <tr>
                <th>线级</th>
                <th>CP</th>
                <th>省份</th>
                <th>单省分非包月付费用户数</th>
                <th>昨日新增</th>
                <th>今日新增</th>
                <th>遗留</th>
                <th>新增万投比</th>
            </tr>
            @if (Model != null)
            {
                foreach (var items in Model)
                {
                    foreach (var item in items.caList)
                    {
                        <tr>
                            <td align="center">
                                @items.SourceLevel
                            </td>
                            <td  align="center">
                                @items.CpName
                            </td>
                            <td  align="center">
                                @item.privnce
                            </td>
                            <td  align="center">
                                @item.notBaoyuePayUserNum
                            </td>
                            <td  align="center">
                                @item.yesterdayAddNum
                            </td>
                            <td  align="center">
                                @item.AddNum
                            </td>
                            <td  align="center">
                                @item.LeaveNum
                            </td>
                            <td align="center">
                                @item.proportion
                            </td>
                        </tr>
                    }
                }
            }
            else
            {
                <tr><td align="center" colspan="8">暂无记录</td></tr>
            }
        </table>
        <!--列表展示.结束-->


        <div class="line15"></div>
        <div class="page_box">
            <div id="PageContent" class="flickr right">@Html.Raw(ViewBag.PageContent)</div>
        </div>
        <div class="line10"></div>
    </form>
</body>
</html>
